pipeline {
  environment {
    registry = "basavarajbhavi/hackathon"
    registryCredential = 'docker_hub_basavarajbhavi'
    dockerImage = ''
  }
  agent any
    tools {nodejs "node"}
  stages{                          
      stage ('Build') {
      steps{
        echo "Building Project"
        sh 'npm install'
        sh 'npm run build'
      }
    }
    
  }
}
